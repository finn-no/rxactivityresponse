<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:id="@+id/rootview"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical">

     <no.finn.rxactivityresponse.sample.SimplePermissionButton
            android:id="@+id/getlocationbuttonwithrationale"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Get permission with rationale"/>

     <no.finn.rxactivityresponse.sample.GpsLocationButton
            android:id="@+id/getlocationwithrationale"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Get location + (enabling locationsettings and permissions) + rationale"/>


    <no.finn.rxactivityresponse.sample.CustomStateObserverExampleButton
            android:id="@+id/getlogintoken"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Get login token"/>

</LinearLayout>

